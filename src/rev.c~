#include <stdio.h>

#include <unistd.h>
#include <stdlib.h>
#include "stu.h"

int rev(int size_read, char *buffer)
{
    int i;
    //int nb;

    //nb = 0;
    i = 0;
    if (!buffer)
        return(0);
    while(i <= size_read) {
        printf("%s\n", buffer);
        i += 1;
    }
}
